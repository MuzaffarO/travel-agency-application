    ],
    "predefined_policies": [],
    "tags": {}
  },
  "booking-status-nightly": {
    "resource_type": "eventbridge_schedule",
    "schedule_content": {
      "name": "solo-booking-status-nightly-dev1",
      "description": "Periodic job to mark finished bookings.",
      "flexible_time_window": {
        "mode": "OFF"
      },
      "schedule_expression": "cron(10 0 * * ? *)",
      "schedule_expression_timezone": "UTC",
      "state": "ENABLED",
      "target": {
        "arn": "arn:aws:lambda:eu-west-3:663264486623:function:solo-booking-status-updater-dev1",
        "role_arn": "arn:aws:iam::663264486623:role/solo-eventbridge-scheduler-role-dev1"
      }
    },
    "dependencies": [
      {
        "resource_name": "eventbridge-scheduler-role",
        "resource_type": "iam_role"
      }
    ]
  },
  "eventbridge-scheduler-invoke-lambda": {
    "resource_type": "iam_policy",
    "policy_content": {
      "Version": "2012-10-17",
      "Statement": [
        {
          "Effect": "Allow",
          "Action": [
            "lambda:InvokeFunction"
          ],
          "Resource": [
            "arn:aws:lambda:eu-west-3:663264486623:function:solo-booking-status-updater-dev1",
            "arn:aws:lambda:eu-west-3:663264486623:function:solo-travel-reports-handler-dev1"
          ]
        }
      ]
    }
  },
  "eventbridge-scheduler-role": {
    "resource_type": "iam_role",
    "principal_service": "scheduler",
    "custom_policies": [
      "eventbridge-scheduler-invoke-lambda"
    ],
    "predefined_policies": []
  },
  "tours": {
    "resource_type": "dynamodb_table",
    "hash_key_name": "tourId",
    "hash_key_type": "S",
    "billing-mode": "PAY_PER_REQUEST",
    "read_capacity": 1,
    "write_capacity": 1,
    "tags": {}
  },
  "travel-agents": {
    "resource_type": "dynamodb_table",
    "hash_key_name": "email",
    "hash_key_type": "S",
