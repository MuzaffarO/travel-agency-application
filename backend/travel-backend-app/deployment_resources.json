  "booking-documents": {
    "resource_type": "s3_bucket",
    "acl": "private",
    "public_access_block": {
      "block_public_acls": true,
      "ignore_public_acls": true,
      "block_public_policy": true,
      "restrict_public_buckets": true
    },
    "LifecycleConfiguration": {
      "Rules": [
        {
          "ID": "ExpireOldDocuments",
          "Status": "Enabled",
          "Expiration": { "Days": 1825 },
          "Filter": { "Prefix": "bookings/" }
        }
      ]
    },
    "tags": {}
  },
  "user-avatars": {
    "resource_type": "s3_bucket",
    "acl": "private",
    "public_access_block": {
      "block_public_acls": true,
      "ignore_public_acls": true,
      "block_public_policy": true,
      "restrict_public_buckets": true
    },
    "LifecycleConfiguration": {
      "Rules": [
        {
          "ID": "ExpiredOldAvatars",
          "Status": "Enabled",
          "Expiration": { "Days": 365 },
          "Filter": { "Prefix": "users/" }
        }
      ]
    },
    "tags": {}
  },
  "reports-sender-schedule": {
    "resource_type": "eventbridge_schedule",
    "schedule_content": {
      "name": "solo-reports-sender-schedule-dev1",
      "description": "Periodic job to generate travel reports every week.",
      "flexible_time_window": {
        "mode": "OFF"
      },
      "schedule_expression": "rate(7 days)",
      "schedule_expression_timezone": "UTC",
      "state": "ENABLED",
      "target": {
        "arn": "arn:aws:lambda:eu-west-3:663264486623:function:solo-travel-reports-handler-dev1",
        "role_arn": "arn:aws:iam::663264486623:role/solo-eventbridge-scheduler-role-dev1",
        "input": "{\"reportType\": \"both\", \"period\": \"WEEKLY\", \"year\": 2025, \"sendEmail\": true}"
      }
    },
    "dependencies": [
      {
        "resource_name": "eventbridge-scheduler-role",
        "resource_type": "iam_role"
      }
    ]
